<script type="text/ng-template" id="material_karcher_form.html">
    <div class="modal-header">
        <h3>Maquina</h3>
    </div>
    <div class="modal-body">
        <fieldset>

            <div class="form-group has-success has-feedback">
                <input
                        ng-model="name"
                        class="form-control"
                        placeholder="Nombre">
            </div>

            <div class="form-group has-success has-feedback">


                <select  class="form-control" name="origenmaterial" id="mySelect1"
                         ng-options="descript.name for descript in materialDescript track by descript.id"
                         ng-model="materialDescriptSelected"></select>

            </div>

            <div class="form-group has-success has-feedback">
                <input
                        ng-model="model"
                        class="form-control"
                        placeholder="Modelo">
            </div>


            <div class="form-group has-success has-feedback">
                <input
                        ng-model="pn"
                        class="form-control"
                        placeholder="Numero de Parte">
            </div>



            <!--<div class="form-group has-success has-feedback">
                <input
                        ng-model="serial_n"
                        class="form-control"
                            placeholder="Numero de Serie">
            </div>-->




            <div class="row">
            <div class="col-lg-6" style="padding-left: 0px!important;">
                <div class="form-group has-success has-feedback">
                        <label>Grupo (Product Mix):</label>

                        <select  class="form-control" name="typematerial" id="mySelect"
                         ng-options="type.name for type in types track by type.id"

                         ng-model="typematerial"></select>
                </div>
            </div>

            <div class="col-lg-6">
                <div class="form-group has-success has-feedback">
                        <label>Origen:</label>

                        <select  class="form-control" name="origenmaterial" id="mySelect1"
                         ng-options="origen.name for origen in origenes track by origen.id"
                         ng-model="origenmaterial"></select>
                </div>

            </div>
            </div>


        </fieldset>


    </div>
    <div class="modal-footer">
        <button type="button" class="btn-karcher-sm pull-right " ng-click="saveMaterial()">Guardar</button>
    </div>
</script>

<script type="text/ng-template" id="modal_orden.html">
    <div class="modal-header">
        <h3>Orden: {{orden.numero}} </h3>
    </div>
    <div class="modal-body">
        <fieldset>

            <div class="row">

                <div class="form-group has-success has-feedback">
                        <label>Tecnico:</label>

                    <select  class="form-control" name="typematerial" id="user"
                             ng-change="changeTec()"
                             ng-options="user.name for user in orderUsersDist track by user.id"
                             ng-model="ordertec"></select>
                </div>
                <div class="form-group has-success has-feedback">
                        <label>Estado:</label>

                        <select  class="form-control" ng-disabled="ordertec.id==0" name="typematerial" id="est"
                                 ng-options="est.name for est in orderEstados track by est.id"
                                 ng-model="orderest"></select>
                </div>
                    <div class="form-group has-success has-feedback">
                        <label>Observaciones:</label>
                           <textarea rows="5" id="obs"  ng-model="orden.obs" class="form-control"></textarea>
                    </div>


            </div>

        </fieldset>


    </div>
    <div class="modal-footer">
    <button type="button" class="btn-karcher-sm pull-left " ng-click="cerrar()">Cerrar</button>
        <button type="button" class="btn-karcher-sm pull-right " ng-click="save()">Guardar</button>
    </div>
</script>

<script type="text/ng-template" id="dist_admin.html">
    <div class="modal-header">
        <h3>Administrador : {{item.admin}}</h3>
    </div>
    <div class="modal-body">
        <div class="row">
            <div class="col-md-4">Email: </div>
            <div class="col-md-8">{{item.mail}}</div>
        </div>

        <div class="row">
            <div class="col-md-4">Telefono: </div>
            <div class="col-md-8">{{item.phone}}</div>
        </div>

        <div class="row">
            <div class="col-md-4">Habilitado: </div>
            <div class="col-md-8" ng-if="item.disabled==1">No</div>
            <div class="col-md-8" ng-if="item.disabled==0">Si</div>

        </div>

    </div>
    <div class="modal-footer">
        <button type="button" class="btn-karcher-sm pull-right " ng-click="cerrar()">Cerrar</button>
    </div>
</script>


{% endverbatim %}