<script type="text/ng-template" id="modal_orden.html">
    <div class="modal-header">
        <h3>Orden: {{orden.numero}} </h3>
    </div>
    <div class="modal-body">
        <fieldset>

            <div class="row">

                <div class="form-group has-success has-feedback">
                        <label>Tecnico:</label>

                    <select  class="form-control" name="typematerial" id="user"
                             ng-change="changeTec()"
                             ng-options="user.name for user in orderUsersDist track by user.id"
                             ng-model="ordertec"></select>
                </div>
                <div class="form-group has-success has-feedback">
                        <label>Estado:</label>

                        <select  class="form-control" ng-disabled="ordertec.id==0" name="typematerial" id="est"
                                 ng-options="est.name for est in orderEstados track by est.id"
                                 ng-model="orderest"></select>
                </div>
                    <div class="form-group has-success has-feedback">
                        <label>Observaciones:</label>
                           <textarea rows="5" id="obs"  ng-model="orden.obs" class="form-control"></textarea>
                    </div>


            </div>

        </fieldset>


    </div>
    <div class="modal-footer">
    <button type="button" class="btn-karcher-sm pull-left " ng-click="cerrar()">Cerrar</button>
        <button type="button" class="btn-karcher-sm pull-right " ng-click="save()">Guardar</button>
    </div>
</script>

<script type="text/ng-template" id="dist_admin.html">
    <div class="modal-header">
