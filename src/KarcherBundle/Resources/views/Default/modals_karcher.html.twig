</script>


<script type="text/ng-template" id="modal_barcode.html">
    <div class="modal-header">
        <h3>Orden : {{value}}</h3>
    </div>
    <div class="modal-body">
        <div class="row" style="height: 100px;text-align: center" id="printable">
        <ng-barcode
            ng-barcode-input="{{ value }}"
            ng-barcode-code="code39"
            ng-barcode-color="{{demoCtrl.colorBarcode()}}"
            ng-barcode-background="{{demoCtrl.colorBackground}}">
        </ng-barcode>
            <label>{{value}}</label>
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn-karcher-sm pull-right " ng-click="print()">Imprimir</button>
        <button type="button" class="btn-karcher-sm pull-left " ng-click="cerrar()">Cerrar</button>
    </div>
</script>

