{# app/Resources/views/base.html.twig #}
{% verbatim %}
<script type="text/ng-template" id="perfil_form.html">
    <div class="modal-header">
        <h3>Perfil</h3>
    </div>
    <div class="modal-body">
        <fieldset>
            <div class="form-group has-success has-feedback">
                <input
                        ng-model="name"
                        class="form-control"
                        placeholder="Nombre(*)">
            </div>


            <div class="form-group has-success has-feedback">
                <input
                        ng-model="descript"
                        class="form-control"
                        placeholder="Descripción(*)">
            </div>
            <hr>

                       <ul class="nav nav-tabs">
                        <li class="active"><a data-toggle="tab" href="perfiles_abm#home"><i class="fa fa-users"></i></a></li>
                        <li><a data-toggle="tab" href="perfiles_abm#menu1"><i class="fa fa-map-marker"></i></a></li>
                        <li><a data-toggle="tab" href="perfiles_abm#menu2"><i class="fa fa-home"></i></a></li>
                        <li><a data-toggle="tab" href="perfiles_abm#menu3"><i class="fa fa-sitemap"></i></a></li>
                        <li><a data-toggle="tab" href="perfiles_abm#menu4"><i class="fa fa-wrench"></i></a></li>
                        <li><a data-toggle="tab" href="perfiles_abm#menu5"><i class="fa fa-area-chart"></i></a></li>
                       </ul>

                       <div class="tab-content">
                         <div id="home" class="tab-pane fade in active">
                           <h3><input ng-model="perfil.user.all" type="checkbox" value=""> Usuarios</h3>
                            <div class="row">
                                <div class="col-lg-4"><input ng-checked="perfil.user.all && perfil.user.abm"  ng-model="perfil.user.abm" type="checkbox" value=""> ABM Usuarios</div>

                                <div class="col-lg-4"><input ng-checked="perfil.user.all && perfil.user.contratistas" ng-model="perfil.user.contratistas" type="checkbox" value=""> ABM Contratistas</div>

                                <div class="col-lg-4"><input ng-checked="perfil.user.all && perfil.user.perfiles" ng-model="perfil.user.perfiles" type="checkbox" value=""> ABM Perfiles</div>
                            </div>
                         </div>
                         <div id="menu1" class="tab-pane fade">
                           <h3><input ng-model="perfil.sitios.all" type="checkbox" value=""> Sitios</h3>
                           <div class="row">
                            <div class="col-lg-6"><input ng-checked="perfil.sitios.all && perfil.sitios.abm" ng-model="perfil.sitios.abm"type="checkbox" value=""> ABM Sitios</div>
                           </div>
                         </div>
                         <div id="menu2" class="tab-pane fade">
                           <h3><input ng-model="perfil.materiales.all" type="checkbox" value=""> Materiales</h3>
                                <div class="row">
                                    <div class="col-lg-6"><input ng-checked="perfil.materiales.all && perfil.materiales.abm_tipo" ng-model="perfil.materiales.abm_tipo" type="checkbox" value=""> ABM Tipo Materiales</div>
                                    <div class="col-lg-6"><input ng-checked="perfil.materiales.all && perfil.materiales.abm" ng-model="perfil.materiales.abm" type="checkbox" value=""> ABM Materiales</div>
                                </div>
                         </div>
                          <div id="menu3" class="tab-pane fade">
                              <h3><input ng-model="perfil.oper.all" type="checkbox" value=""> Manejo de Stock</h3>
                              <div class="row">
                                <div class="col-lg-4"><input ng-checked="perfil.oper.all && perfil.oper.stock" ng-model="perfil.oper.stock" type="checkbox" value=""> Stock</div>
                                <div class="col-lg-4"><input ng-checked="perfil.oper.all && perfil.oper.stock_to_tec" ng-model="perfil.oper.stock_to_tec" type="checkbox" value=""> Maestro a Técnico</div>
                                <div class="col-lg-4"><input ng-checked="perfil.oper.all && perfil.oper.stock_tec_to_tec" ng-model="perfil.oper.stock_tec_to_tec" type="checkbox" value=""> Técnico a Técnico</div>
                              </div>

                            </div>
                              <div id="menu4" class="tab-pane fade">
                                <h3><input type="checkbox" value="" ng-model="perfil.opertec.all"> Operaciones</h3>
                                    <div class="row">
                                     <div class="col-lg-2"><input ng-checked="perfil.opertec.all && perfil.opertec.stock" ng-model="perfil.opertec.stock" type="checkbox" value=""> Stock</div>
                                     <div class="col-lg-3"><input ng-checked="perfil.opertec.all && perfil.opertec.stoc_pend" ng-model="perfil.opertec.stoc_pend" type="checkbox" value=""> Aceptación de Stock</div>
                                     <div class="col-lg-3"><input ng-checked="perfil.opertec.all && perfil.opertec.stoc_tec" ng-model="perfil.opertec.stoc_tec" type="checkbox" value=""> Stock a Técnico</div>
                                     <div class="col-lg-3"><input ng-checked="perfil.opertec.all && perfil.opertec.stoc_sit" ng-model="perfil.opertec.stoc_sit" type="checkbox" value=""> Stock a Sitio</div>
                                    </div>
                                </div>
                              <div id="menu5" class="tab-pane fade">
                              <h3><input type="checkbox" value="" ng-model="perfil.reportes.all">  Reportes</h3>
                                 <div class="row">
                                    <div class="col-lg-3"><input ng-checked="perfil.reportes.all && perfil.reportes.stock_maestro" ng-model="perfil.reportes.stock_maestro" type="checkbox" value=""> Stock Maestro</div>
                                    <div class="col-lg-3"><input ng-checked="perfil.reportes.all && perfil.reportes.stock_tec" ng-model="perfil.reportes.stock_tec" type="checkbox" value=""> Stock por Técnico</div>
                                    <div class="col-lg-3"><input ng-checked="perfil.reportes.all && perfil.reportes.stock_sit" ng-model="perfil.reportes.stock_sit" type="checkbox" value=""> Stock por Sitio</div>
                                    <!--<div class="col-lg-3"><input ng-checked="perfil.reportes.all && perfil.reportes.stock_cont" ng-model="perfil.reportes.stock_cont" type="checkbox" value=""> Stock por Contratista</div>-->
                                    <div class="col-lg-3"><input ng-checked="perfil.reportes.all && perfil.reportes.stock_mov" ng-model="perfil.reportes.stock_mov" type="checkbox" value=""> Movimientos de Stock</div>
                                    <div class="col-lg-3"><input ng-checked="perfil.reportes.all && perfil.reportes.stock_mov_by_mat" ng-model="perfil.reportes.stock_mov_by_mat" type="checkbox" value=""> Movimientos de Stock por Material</div>

                                    <div class="col-lg-3"><input ng-checked="perfil.reportes.all && perfil.reportes.stock_alertas" ng-model="perfil.reportes.stock_alertas" type="checkbox" value=""> Alertas de Stock</div>
                                    <div class="col-lg-3"><input ng-checked="perfil.reportes.all && perfil.reportes.compras" ng-model="perfil.reportes.compras" type="checkbox" value=""> Historial de Compras</div>
                            </div>
                       </div>
            </div>


        </fieldset>


    </div>
    <div class="modal-footer">
        <a class="btn btn-success" ng-click="saveProfile()">Guardar</a>
    </div>
</script>

