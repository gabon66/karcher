<script type="text/ng-template" id="moda_alta_stock.html">
    <div class="modal-header">
         <div class="row">
            <div class="col-md-10"> <h3>Alta de Stock {{item.idCustom}} - {{ item.name }} </h3></div>
            <div class="col-md-2"><a class="btn btn-warning pull-right" ng-click="cerrar()">Cerrar</a> </div>
         </div>

    </div>
    <div class="modal-body">
        <div ng-show="!historial">
            <div class="row">
                <div class="col-lg-3">
                    <div class="form-group has-success has-feedback">

                       <label>Stock Entrante :</label>
                            <input
                             ng-model="stock"
                             type="number"
                             class="form-control"
                             placeholder="0">

                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="form-group has-success has-feedback">
                        <label>Origen :</label>
                        <select  class="form-control" name="origenmaterial" id="mySelect"
                            ng-options="contratista.name for contratista in contratistas track by contratista.id"
                         ng-model="contratistaselected"></select>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="form-group has-success has-feedback">
                            <label>Fecha Compra :</label><br>
                            <datepicker date-format="dd-MM-yyyy" >
                                <input class="form-control" ng-model="date" type="text"/>
                            </datepicker>

                    </div>
                </div>
                <div class="col-lg-1"></div>
                <div class="col-lg-12">
                <label>Observación:</label>
                            <textarea class="form-control" ng-model="obs" rows="2"></textarea>
                </div>
            </div>
        </div>
        <div ng-show="historial">
                   <div class="row">
                <div class="col-lg-12">
                    <lable>Historial de cargas :</lable>
                <div style="max-height: 500px;overflow: auto">
                <table class="table table-bordered table-inmodal">
                    <thead>
                    <tr>
                        <th>Fecha</th>
                        <th>Contratista</th>
                        <th>Stock Entrante</th>
                        <th>Observación</th>
                        <th>Guardar</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr ng-repeat="alta in altas ">
                        <td>
                          <!--<md-datepicker ng-model="alta.date_obj"></md-datepicker>-->
                            <datepicker date-format="dd-MM-yyyy" >
                                <input class="form-control" ng-model="alta.date_obj" type="text"/>
                            </datepicker>
                        </td>
                        <td>
                            <select  class="form-control" name="origenmaterial" id="mySelect"
                                ng-options="contratista.name for contratista in contratistas track by contratista.id"
                                ng-model="alta.provselected"></select>
                        </td>
                        <td>{{ alta.stock }}</td>
                        <td>
                            <textarea class="form-control" ng-model="alta.obs" rows="2"></textarea>
                        </td>
                        <td><a  class="btn btn-sm btn-add btn-success" ng-click="save(alta)" ><i class="fa fa-floppy-o" aria-hidden="true"></i></a> </td>
                    </tr>
                    </tbody>
                </table>
                </div>


                </div>
            </div>
        </div>

    </div>

    <div class="modal-footer">
        <a ng-if="!historial" class="btn btn-success pull-left"  ng-click="showHistorial(true)">Ver Historial</a>
        <a ng-if="historial" class="btn btn-success pull-left"  ng-click="showHistorial(false)">Volver</a>

        <a ng-if="!historial" class="btn btn-success" ng-click="asignar()">Asignar</a>
    </div>
</script>


